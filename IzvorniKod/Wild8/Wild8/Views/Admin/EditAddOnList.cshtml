@model IEnumerable<AddOn>

@{ 
    Layout = null;
}
<div class="container-fluid" id="edit-meal-list">
<div class="panel panel-default">
    <div class="panel-heading">
        <h3>Popis dodataka</h3>
    </div>
    <div class="panel-body">
        <ul class="list-group">
            @foreach (var item in Model)
            {
                <li class="list-group-item">
                    @item.AddOnID
                    <span class="pull-right">
                        <button type="button" class="btn btn-warning btn-sm edit-addon-btn" data-url="@Url.Action("EditAddOn", "Admin")" data-addon-id="@item.AddOnID">Uredi dodatak</button>
                    </span>
                </li>
            }
        </ul>
    </div>
</div>    
    <script type="text/javascript" src="~/Scripts/ViewScripts/Admin_View_Scripts/EditAddOnListScript.js"></script>
</div>